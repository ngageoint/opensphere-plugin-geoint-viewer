{
  "name": "opensphere-plugin-geoint-viewer",
  "version": "1.4.0",
  "overrideVersion": "1.4.0",
  "description": "OpenSphere",
  "directories": {
    "externs": "externs",
    "src": "src",
    "scss": "scss",
    "views": "views"
  },
  "build": {
    "type": "plugin",
    "config": ["config/settings.json", "config/bits-internal-override.json"],
    "scss": "scss/gv.scss",
    "gcc": {
      "entry_point": [
        "goog:osngaimports"
      ]
    },
    "themes": [
      "nga-dark",
      "nga-dark-cb_b",
      "nga-dark-cb_rg",
      "nga-light",
      "nga-light-cb_b",
      "nga-light-cb_rg"
    ]
  },
  "scripts": {
    "build": "npm run lint",
    "lint": "run-p lint:css lint:html lint:js",
    "lint:css": "sass-lint -v -s scss 'scss/**/*.scss'",
    "lint:html": "html-validate 'views/**/*.html'",
    "lint:js": "eslint --cache 'src/**/*.js'"
  },
  "keywords": [
    "gis",
    "GEOINT",
    "geospatial",
    "opensphere"
  ],
  "author": "BIT Systems",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "eslint": "^6.0.0",
    "eslint-config-opensphere": "^4.0.0",
    "html-validate": "^4.2.0",
    "html-validate-angular": "^2.9.0",
    "husky": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "sass-lint": "^1.12.1"
  },
  "dependencies": {
    "opensphere": "0.0.0-development",
    "opensphere-nga-lib": "^1.0.0",
    "opensphere-plugin-geopackage": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}

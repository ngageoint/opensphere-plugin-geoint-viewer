{
  "admin": {
    "banner": {
      "before": ".js-navtop",
      "after": ".js-navbottom",
      "markup": "<div class=\"c-banner\" style=\"background:#060;color:#FFF;\"><div>UNCLASSIFIED</div></div>"
    },
    "crossOrigin": [{
      "pattern": "^https?://pixia.geointservices.io(:\\d+)?/",
      "crossOrigin": "use-credentials"
    }, {
      "pattern": "^https?://earth.gvslabs.com/terrain/",
      "crossOrigin": "use-credentials"
    }],
    "plugin": {
      "login": {
        "geoaxis": {
          "loginUrl": "https://geoaxis.nga.mil",
          "regexes": [
            "<(html|body)(\\s+|>)",
            "<title>GEOAxIS</title>",
            "<form name=\"login\""
          ]
        }
      }
    },
    "providers": {
      "gs-hiperlook": {
        "type": "ogc",
        "label": "GEOINT Services Hiperlook",
        "wms": "https://pixia.stage.gs.mil/wmsserver/",
        "wmsParams": "version=1.1.1&map=default.map"
      }
    },
    "weather": {
      "url": "http://forecast.weather.gov/MapClick.php?lat={lat}&lon={lon}"
    }
  },
  "user": {
    "baseProjection": "EPSG:4326"
  }
}

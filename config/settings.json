{
  "admin": {
    "about": {
      "application": "OpenSphere",
      "logoPath": "images/brand/logo/opensphere.svg",
      "appVendors": [
        {
          "logoPath": "images/vendor/opensphere.png",
          "name": "OpenSphere"
        },
        {
          "logoPath": "images/vendor/openlayers.png",
          "name": "OpenLayers"
        },
        {
          "logoPath": "images/vendor/cesium.png",
          "name": "Cesium"
        },
        {
          "name": "AngularJS",
          "logoPath": "images/vendor/angular.png"
        },
        {
          "name": "Closure",
          "logoPath": "images/vendor/closure.png"
        },
        {
          "name": "Bootstrap",
          "logoPath": "images/vendor/bootstrap.png"
        },
        {
          "name": "jQuery",
          "logoPath": "images/vendor/jquery.png"
        }
      ]
    },
    "banner": {
      "before": ".js-navtop",
      "after": ".js-navbottom",
      "markup": "<div class=\"c-banner\" style=\"background:#060;color:#FFF;\">UNCLASSIFIED</div>"
    },
    "crossOrigin": {
      "assume-anon-ssl": {
        "pattern": "^https://",
        "value": "anonymous",
        "priority": -999
      },
      "mil": {
        "pattern": "^https://[^/]*\\.mil(:\\d+)?/",
        "value": "use-credentials",
        "priority": -900
      },
      "nominatim": {
        "pattern": "^https://osm-nominatim\\.gs\\.mil/",
        "value": "anonymous"
      },
      "osm-ogc": {
        "pattern": "^https://osm\\.gs\\.mil/(ows|tiles|wmts)",
        "value": "anonymous"
      }
    },

    "plugin": {
      "login": {
        "geoaxis": {
          "loginUrl": "https://geoaxis.nga.mil",
          "regexes": [
            "<(html|body)(\\s+|>)",
            "<title>GEOAxIS</title>",
            "<form name=\"login\""
          ]
        }
      }
    },
    "plugins": {
      "piwik": false
    },
    "providers": {
      "gs-hiperlook": {
        "type": "ogc",
        "label": "GEOINT Services Hiperlook",
        "wms": "https://pixia.stage.gs.mil/wmsserver/",
        "wmsParams": "version=1.1.1&map=default.map"
      }
    },
    "themes": {
      "Default": "nga-dark",
      "Light": "nga-light",
      "Dark": "nga-dark"
    },
    "accessible_themes": {
      "Default": {
        "Red & Green Deficiency (Protanomaly & Deuteranomaly)": "nga-dark-cb_rg",
        "Blue Deficiency (Tritanopia)": "nga-dark-cb_b"
      },
      "Light": {
        "Red & Green Deficiency (Protanomaly & Deuteranomaly)": "nga-light-cb_rg",
        "Blue Deficiency (Tritanopia)": "nga-light-cb_b"
      },
      "Dark": {
        "Red & Green Deficiency (Protanomaly & Deuteranomaly)": "nga-dark-cb_rg",
        "Blue Deficiency (Tritanopia)": "nga-dark-cb_b"
      }
    },
    "weather": {
      "url": "http://forecast.weather.gov/MapClick.php?lat={lat}&lon={lon}"
    },
    "os": {
      "auth": {
        "sage": {
          "title": "NORTHCOM SAGE ArcGIS",
          "tooltip": "The NORTHCOM Sage ArcGIS Server provides additional data layers when accessed from an SBU machine and authenticated. Click here to go to the login page.",
          "message": "The NORTHCOM Sage ArcGIS Server provides additional data layers when accessed from an SBU machine.",
          "pattern": "https://sageearth.northcom.mil",
          "link": "https://sageearth.northcom.mil/UserManagement/aeiss/newAeissUser.do/"
        },
        "ngaEnterprise": {
          "title": "NGA ACES ArcGIS",
          "tooltip": "The NGA ACES ArcGIS Server requires PKI approval before layers are accessible. Click here to go to the login page.",
          "message": "Layers provided by the NGA ACES ArcGIS Server require authentication before use.",
          "pattern": "(NGA Aces|https://aerodata.nga.mil)",
          "link": "https://pki.geo.nga.mil/"
        }
      }
    }
  },
  "user": {
    "baseProjection": "EPSG:4326",
    "os": {
      "map": {
        "cameraMode": "last"
      }
    }
  }
}

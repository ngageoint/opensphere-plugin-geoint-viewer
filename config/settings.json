{
  "admin": {
    "about": {
      "application": "GEOINT Viewer",
      "appVendors": [
        {
          "logoPath": "images/opensphere.png",
          "name": "Open Sphere"
        },
        {
          "logoPath": "images/vendor/ol3.png",
          "name": "OpenLayers3"
        },
        {
          "logoPath": "images/vendor/cesium.png",
          "name": "Cesium"
        },
        {
          "name": "AngularJS",
          "logoPath": "images/vendor/angular.png"
        },
        {
          "name": "Closure",
          "logoPath": "images/vendor/closure.png"
        },
        {
          "name": "Bootstrap",
          "logoPath": "images/vendor/bootstrap.png"
        },
        {
          "name": "jQuery",
          "logoPath": "images/vendor/jquery.png"
        }
      ]
    },
    "banner": {
      "before": "div.tr",
      "after": "div.tr",
      "markup": "<div class=\"tr banner\" style=\"background:#060;color:#FFF;\"><div>UNCLASSIFIED</div></div>"
    },
    "crossOrigin": [{
      "pattern": "^https?://hiperlook.geointservices.io(:\\d+)?/",
      "crossOrigin": "use-credentials"
    }],
    "providers": {
      "gs-hiperlook": {
        "type": "ogc",
        "label": "GEOINT Services Hiperlook",
        "wms": "https://hiperlook.geointservices.io/wmsserver/",
        "wmsParams": "version=1.1.1&map=default.map"
      }
    },
    "weather": {
      "url": "http://forecast.weather.gov/MapClick.php?lat={lat}&lon={lon}"
    }
  },
  "user": {
    "baseProjection": "EPSG:4326"
  }
}

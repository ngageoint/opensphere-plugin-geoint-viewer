{
  "admin": {
    "about": {
      "application": "GEOINT Viewer",
      "logoPath": "images/logo.svg",
      "appVendors": [
        {
          "logoPath": "images/opensphere.png",
          "name": "Open Sphere"
        },
        {
          "logoPath": "images/vendor/ol3.png",
          "name": "OpenLayers3"
        },
        {
          "logoPath": "images/vendor/cesium.png",
          "name": "Cesium"
        },
        {
          "name": "AngularJS",
          "logoPath": "images/vendor/angular.png"
        },
        {
          "name": "Closure",
          "logoPath": "images/vendor/closure.png"
        },
        {
          "name": "Bootstrap",
          "logoPath": "images/vendor/bootstrap.png"
        },
        {
          "name": "jQuery",
          "logoPath": "images/vendor/jquery.png"
        }
      ]
    },
    "banner": {
      "before": ".navbar",
      "after": ".navbar",
      "markup": "<div class=\"banner\" style=\"background:#060;color:#FFF;\"><div>UNCLASSIFIED</div></div>"
    },
    "crossOrigin": [{
      "pattern": "^https?://pixia.geointservices.io(:\\d+)?/",
      "crossOrigin": "use-credentials"
    }, {
      "pattern": "^https?://earth.gvslabs.com/terrain/",
      "crossOrigin": "use-credentials"
    }],
    "plugin": {
      "login": {
        "geoaxis": {
          "loginUrl": "https://geoaxis.nga.mil",
          "regexes": [
            "<(html|body)(\\s+|>)",
            "<title>GEOAxIS</title>",
            "<form name=\"login\""
          ]
        },
        "default": {
          "priority": -1000,
          "regexes": [
            "<(html|body)(\\s+|>)",
            "(sign|log)\\s*in)"
          ]
        }

      }
    },
    "providers": {
      "gs-hiperlook": {
        "type": "ogc",
        "label": "GEOINT Services Hiperlook",
        "wms": "https://pixia.geointservices.io/wmsserver/",
        "wmsParams": "version=1.1.1&map=default.map"
      }
    },
    "weather": {
      "url": "http://forecast.weather.gov/MapClick.php?lat={lat}&lon={lon}"
    }
  },
  "user": {
    "baseProjection": "EPSG:4326"
  }
}

{
  "admin": {
    "about": {
      "application": "OpenSphere",
      "logoPath": "images/brand/logo/opensphere.svg",
      "appVendors": [
        {
          "logoPath": "images/vendor/opensphere.png",
          "name": "OpenSphere"
        },
        {
          "logoPath": "images/vendor/openlayers.png",
          "name": "OpenLayers"
        },
        {
          "logoPath": "images/vendor/cesium.png",
          "name": "Cesium"
        },
        {
          "name": "AngularJS",
          "logoPath": "images/vendor/angular.png"
        },
        {
          "name": "Closure",
          "logoPath": "images/vendor/closure.png"
        },
        {
          "name": "Bootstrap",
          "logoPath": "images/vendor/bootstrap.png"
        },
        {
          "name": "jQuery",
          "logoPath": "images/vendor/jquery.png"
        }
      ]
    },
    "banner": {
      "before": ".js-navtop",
      "after": ".js-navbottom",
      "markup": "<div class=\"c-banner c-banner-u\">UNCLASSIFIED</div>"
    },
    "crossOrigin": {
      "assume-anon-ssl": {
        "pattern": "^https://",
        "value": "anonymous",
        "priority": -999
      },
      "mil": {
        "pattern": "^https://[^/]*\\.mil(:\\d+)?/",
        "value": "use-credentials",
        "priority": -900
      },
      "nominatim": {
        "pattern": "^https://osm-nominatim\\.gs\\.mil/",
        "value": "anonymous"
      },
      "osm-ogc": {
        "pattern": "^https://osm\\.gs\\.mil/(ows|tiles|wmts)",
        "value": "anonymous"
      }
    },

    "plugin": {
      "login": {
        "geoaxis": {
          "loginUrl": "https://geoaxis.nga.mil",
          "regexes": [
            "<(html|body)(\\s+|>)",
            "<title>GEOAxIS</title>",
            "<form name=\"login\""
          ]
        }
      }
    },
    "plugins": {
      "piwik": false
    },
    "providers": {
      "gs-hiperlook": {
        "type": "ogc",
        "label": "GEOINT Services Hiperlook",
        "wms": "https://pixia.stage.gs.mil/wmsserver/",
        "wmsParams": "version=1.1.1&map=default.map"
      }
    },
    "themes": {
      "Default": "nga-dark",
      "Light": "nga-light",
      "Dark": "nga-dark"
    },
    "accessible_themes": {
      "Default": {
        "Red & Green Deficiency (Protanomaly & Deuteranomaly)": "nga-dark-cb_rg",
        "Blue Deficiency (Tritanopia)": "nga-dark-cb_b"
      },
      "Light": {
        "Red & Green Deficiency (Protanomaly & Deuteranomaly)": "nga-light-cb_rg",
        "Blue Deficiency (Tritanopia)": "nga-light-cb_b"
      },
      "Dark": {
        "Red & Green Deficiency (Protanomaly & Deuteranomaly)": "nga-dark-cb_rg",
        "Blue Deficiency (Tritanopia)": "nga-dark-cb_b"
      }
    },
    "weather": {
      "url": "http://forecast.weather.gov/MapClick.php?lat={lat}&lon={lon}"
    },
    "os": {
      "auth": {
        "sage": {
          "title": "NORTHCOM SAGE ArcGIS",
          "tooltip": "Click here to go to the NORTHCOM SAGE ArcGIS login page.",
          "message": "The NORTHCOM Sage ArcGIS Server provides additional data layers when accessed from an SBU machine and authenticated with their services. Click the links below to create your account and go to the login page.<br><br><a href=\"https://sageearth.northcom.mil/UserManagement/aeiss/newAeissUser.do/\" target=\"_blank\">Click here to go to the New User Account Request page</a>",
          "pattern": "https://sageearth.northcom.mil",
          "link": "https://sageearth.northcom.mil/hosting/login"
        },
        "ngaEnterprise": {
          "title": "NGA ACES Geoserver",
          "tooltip": "Click here to go to the NGA ACES Geoserver PKI approval request page.",
          "message": "Layers provided by the NGA ACES Geoserver require that your PKI certificate be registered and approved before you will be able to load data. Click the link below to go to the PKI approval request page.",
          "pattern": "(NGA Aces|https://aerodata.nga.mil)",
          "link": "https://pki.geo.nga.mil/"
        }
      },
      "layers": {
        "defaults": {
          "y": 74
        }
      }
    }
  },
  "user": {
    "baseProjection": "EPSG:4326",
    "os": {
      "map": {
        "cameraMode": "last"
      }
    }
  }
}
